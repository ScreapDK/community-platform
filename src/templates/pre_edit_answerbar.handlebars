<div class="row-diff" id="row-diff-answerbar">
    <div class="column-left-labels left">
        <div class="column-left-labels__child">
            <p class="column-left-labels__child__p">
                Fallback Timeout (ms)
            </p>
        </div>
    </div>
    <div class="column-center-live left">
        <div class="column-center-live__child">
            <p class="column-center-live__child__p">
                {{#if live.answerbar.fallback_timeout}}
                    {{live.answerbar.fallback_timeout}}
                {{else}}
                    â€”
                {{/if}}
            </p>
        </div>
    </div>
    <div class="column-right-edits left" id="column-edits-answerbar">
        <div class="column-right-edits__child" id="answerbar" name="answerbar">
            <p class="column-right-edits__child__p">
                {{edited.answerbar.fallback_timeout}}
            </p>
        </div>
    </div>

    <div class="button js-pre-editable" name="answerbar">
        <i class="js-pre-editable__icon icon-edit"></i>
        <span class="js-pre-editable__text">Edit</span>
    </div>

    <div class="button hide js-editable" name="answerbar">
        <i class="js-editable__icon icon-check"></i>
        <span class="js-editable__text">Done</span>
    </div>
</div>
