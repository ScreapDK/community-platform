{{#eq live.repo 'longtail'}}
<div class="row-diff" id="row-diff-is_stackexchange">
    <div class="column-left-labels left">
        <div class="column-left-labels__child">
            <p class="column-left-labels__child__p">
                Stack Exchange
            </p>
        </div>
    </div>
    <div class="column-center-live left">
        <div class="column-center-live__child">
            <p class="column-center-live__child__p">
                <i class="icon-check{{#is_false live.is_stackexchange}}-empty{{/is_false}}"/>
                Is Stack Exchange
            </p>
        </div>
    </div>
    <div class="column-right-edits left" id="column-edits-is_stackexchange">
        <div class="column-right-edits__child" id="is_stackexchange" name="is_stackexchange">
            <p class="column-right-edits__child__p">
                {{#not_null edited.is_stackexchange}}
                    <i class="icon-check{{#is_false edited.is_stackexchange}}-empty{{/is_false}}"/>
                    Is Stack Exchange
                {{/not_null}}
            </p>
        </div>
    </div>

    <div class="button js-pre-editable" name="is_stackexchange">
        <i class="js-pre-editable__icon icon-edit"></i>
        <span class="js-pre-editable__text">Edit</span>
    </div>

    <div class="button hide js-editable js-check" name="is_stackexchange">
        <i class="js-editable__icon icon-check"></i>
        <span class="js-editable__text">Done</span>
    </div>
</div>
{{/eq}}
