<div class="content-box account-box">
	<div class="head">
		<h2><a href="<: $u($_.u) :>">
			<: $_.name :>
		</a></h2>
		<h2 class="pull-right">
			<a href="<: $u('Duckpan','release_all',$_.name) :>">
				Show all versions
			</a>
			<: $_.version :>
		</h2>
	</div>
	<div class="body">
		<div class="row">
			<: for $_.duckpan_modules.all_ref -> $duckpan_module { :>
				<: $duckpan_module.name :>
				<: if !$~duckpan_module.is_last { :>&nbsp;|&nbsp;<: } :>
			<: } :>
		</div>
	</div>
</div>
