<h3>Back soon...</h3>
: if 0 {
<: if $github_user { :>
  <: i($github_user,'view') :>
  <: if cur_user().admin { :>
    <: i($github_user,'repos') :>
  <: } :>
<: } else { :>
  <a class="noblank button blue" href="https://github.com/login/oauth/authorize?client_id=<: $github_client_id :>&redirect_uri=<: $c.req.uri | uri :>">
    Link your GitHub account
  </a>
<: } :>
: }
